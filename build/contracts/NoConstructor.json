  "source": "//SPDX-License-Identifier: MIT\n\npragma solidity 0.8.7;\n\ncontract Factory {\n    event Deployed(address _addr);\n    function deploy(uint salt, bytes calldata bytecode) public {\n        bytes memory implInitCode = bytecode;\n        address addr;\n        assembly {\n            let encoded_data := add(0x20, implInitCode) // Load init code\n            let encoded_size := mload(implInitCode) // Load init codes length\n            addr := create2(0, encoded_data, encoded_size, salt)\n        }\n        emit Deployed(addr);\n    }\n}\n\ncontract NoConstructor {\n    uint public myUint = 5;\n}",
